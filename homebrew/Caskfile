# Install native apps
# Usage: `brew bundle Caskfile`

install caskroom/cask/brew-cask
tap caskroom/versions

cask install dropbox --force 2> /dev/null
cask install google-chrome --force 2> /dev/null
cask install iterm2 --force 2> /dev/null
cask install sublime-text3 --force 2> /dev/null
cask install virtualbox --force 2> /dev/null

cask install visualvm --force 2> /dev/null
cask install intellij-idea --force 2> /dev/null
cask install pgadmin3 --force 2> /dev/null

# quicklook plugins
cask install quicklook-json --force 2> /dev/null
cask install quicklook-csv --force 2> /dev/null
cask install qlcolorcode --force 2> /dev/null
cask install qlmarkdown --force 2> /dev/null
cask install qlprettypatch --force 2> /dev/null
cask install betterzipql --force 2> /dev/null

cask install alfred --force 2> /dev/null
cask install adium --force 2> /dev/null
cask install rdio --force 2> /dev/null

cask install caskroom/versions/java6 --force 2> /dev/null
cask install caskroom/versions/java7 --force 2> /dev/null
cask install java --force 2> /dev/null

cask install hipchat --force 2> /dev/null
cask install caffeine --force 2> /dev/null
cask install evernote --force 2> /dev/null
cask install sourcetree --force 2> /dev/null

cask install switchresx --force 2> /dev/null

cask cleanup
cask list
